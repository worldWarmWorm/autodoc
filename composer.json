{
  "name": "world-warm-worm/autodoc",
  "description": "Generates documentation of classes  methods in several file formats",
  "type": "library",
  "homepage": "https://github.com/worldWarmWorm/autodoc",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "v.davydkin",
      "email": "world-warm-worm@ya.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.6.x-dev",
    "phpstan/phpstan": "2.1.x-dev",
    "phpstan/phpstan-strict-rules": "2.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Autodoc\\": "src/"
    },
    "files": ["src/functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Autodoc\\Tests\\": "tests/"
    }
  }
}
